{
  "name": "esm-multi-loader",
  "version": "1.2.1",
  "description": "ESM loader for Babel, similar to @babel/register",
  "type": "module",
  "exports": "./src/esm-multi-loader.js",
  "main": "./src/esm-multi-loader.js",
  "types": "./types/src/esm-multi-loader.d.ts",
  "engines": {
    "node": ">=12.17.0"
  },
  "files": [
    "src",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "concurrently -r 'npm:build:*'",
    "build:typescript": "rm -rf types && tsc && cp src/*.d.ts types/src",
    "test": "concurrently -r 'npm:test:*'",
    "test:mocha-parallel": "npm run mocha -- --parallel",
    "test:eslint": "eslint --color --max-warnings=0 '**/*.js'",
    "test:typescript": "tsc --noEmit --emitDeclarationOnly false --pretty",
    "mocha": "mocha --no-timeouts --exit --color 'test/unit/*.test.js' 'test/e2e/*.test.js'"
  },
  "keywords": [
    "javascript",
    "nodejs"
  ],
  "author": "Gil Tayar <gil@tayar.org>",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/giltayar/esm-multi-loader",
    "type": "git"
  },
  "peerDependencies": {
    "@babel/core": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/plugin-syntax-top-level-await": "^7.14.5",
    "@babel/plugin-transform-react-jsx": "^7.16.5",
    "@babel/plugin-transform-typescript": "^7.16.1",
    "@types/babel__core": "^7.1.17",
    "@types/chai": "^4.3.0",
    "@types/chai-subset": "^1.3.3",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.4",
    "chai": "^4.3.4",
    "chai-subset": "^1.6.0",
    "concurrently": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "execa": "^6.0.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "quibble": "^0.6.6",
    "react": "^17.0.2",
    "typescript": "^4.5.4"
  },
  "dependencies": {
  }
}
